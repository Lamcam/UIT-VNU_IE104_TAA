<span class="btn-icon btn-radio <%= classList %>">
  <input type="radio" id="<%= id %>" name="<%= name %>" value="<%= value %>" <%= (checked) ? 'checked' : '' %>>
  <label for="<%= id %>">
    <span class="icon material-symbols-outlined --unchecked">
      radio_button_unchecked</span>
    <span class="icon material-symbols-outlined --checked">
      radio_button_checked</span>
  </label>
</span>